{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/center/index",
      "style": {
        "navigationBarTitleText": "个人中心"
      }
    },
    {
      "path": "pages/center/coupon-center/index",
      "style": {
        "navigationBarTitleText": "优惠券"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/my-prize/index",
      "style": {
        "navigationBarTitleText": "我的奖品"
      }
    },
    {
      "path": "pages/my-prize/prize-detail",
      "style": {
        "navigationBarTitleText": "我的奖品",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/my-prize/store-list",
      "style": {
        "navigationBarTitleText": "选择门店",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/member-equity/index",
      "style": {
        "navigationBarTitleText": "会员权益"
      }
    },
    {
      "path": "pages/member-equity/growth-details",
      "style": {
        "navigationBarTitleText": "我的成长值"
      }
    },
    {
      "path": "pages/integral/index",
      "style": {
        "navigationBarTitleText": "我的积分"
      }
    }
  ],
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/center/index",
        "text": "个人中心"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#FFF",
    "backgroundColor": "#F5F5F5"
  },
  "subPackages": [
    {
      "root": "points-mall-pages",
      "pages": [
        {
          "path": "address/address-list",
          "style": {
            "navigationBarTitleText": "收货地址"
          }
        },
        {
          "path": "address/add",
          "style": {
            "navigationBarTitleText": "添加地址"
          }
        }
      ]
    }
  ]
}
